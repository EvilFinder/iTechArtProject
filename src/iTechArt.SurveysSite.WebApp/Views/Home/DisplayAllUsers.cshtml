@model IReadOnlyCollection<iTechArt.SurveysSite.DomainModel.User>
@{
    ViewData["Title"] = "DisplayAll";
}
    
<div>
    @if (Model.Count == 0)
    {
        <h1>No users yet</h1>
    }
    else
    {
        <ul>
            @foreach (var user in Model)
            {
                <li>User id: @user.Id, Username: @user.FullName</li>
            }
        </ul> 
    }
</div>


